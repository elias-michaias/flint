import C "stdio.h" as stdio

// Test file for unification constraints
add :: (i32, i32) -> i32
add :: ($x, $y) => $x + $y

main :: () -> () using C
main :: () => { 
    let add($z, 2) = 11
    let add($a, $z) = 15
    let $test = add(~$a, ~$a)
    C.stdio.printf("Result: a = %d, z = %d, test = %d\n", $a, $z, $test)
}
